<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sf2="sap.ui.layout.form">
	<Dialog id="diaologUp" title="Edit Details" titleAlignment="Center">
		<content>
			<sf2:SimpleForm id="simpleFormFragment" editable="false" layout="ResponsiveGridLayout" adjustLabelSpan="false" emptySpanL="6" columnsL="1">
				<Label text="Emp Id"></Label>
				<Input value="{Empno}" id="empTextId3" submit="onEdit" editable="false"/>
				<Label text="Emp Name"></Label>
				<Input value="{Empname}" id="setTextId3" editable="true"/>
				<Label text="Emp Designation"></Label>
				<Input value="{Empdesig}" id="setDesintionId3" editable="true"/>
				<Label></Label>
				<Button text="Save" press="onSaveUpdate"></Button>
			</sf2:SimpleForm>
		</content>
		<beginButton>
			<Button text="Close" press="onClosesUpdate"></Button>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>